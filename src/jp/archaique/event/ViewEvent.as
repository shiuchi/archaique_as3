package jp.archaique.event
{
	import flash.events.Event;

	public class ViewEvent extends Event
	{
		
		public static var SHOW:String="show";
		public static var HIDE:String="hide";
		public static var SHOWED:String="showed";
		public static var HID:String="hid";
		
		
		public function ViewEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
		
	}
}